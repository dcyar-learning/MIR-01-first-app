doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    title Hello
  body
    table
      thead
        th Id
        th Name
        th Visits 
      tbody
        each visitor in visitors
          tr
            td #{visitor._id}
            td #{visitor.name}
            td #{visitor.count}