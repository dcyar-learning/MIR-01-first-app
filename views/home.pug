doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    title Formulario de Registro
  body
    a(href="/logout") Salir
    
    table(border="1")
      thead
        tr
          th Name
          th Email
      tbody
        each user in users
          tr
            td #{user.name}
            td #{user.email}